{
    "name": "I am here",
    "version": "1.0",
    "description": "Build an Extension!", 
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["myscript.js"]
        }
      ],
      "chrome_url_overrides":{
          "newtab" : "newtab.html"
      },
      "background":{
          "scripts": ["background.js"]
      },
    "browser_action": {
        "default_popup": "recChec.html"
    },
    "manifest_version": 2
  }