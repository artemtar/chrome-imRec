{
    "manifest_version": 2,
    "name": "FearBlocker",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "web_accessible_resources": [
        "javascript-lemmatizer/bower_components/underscore/*.json",
        "javascript-lemmatizer/dict/*.json"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "js/jquery-3.4.1.min.js",
                "js/compromise.min.js",
                "js/natural.js",
                "js/removeStopWords.js",
                "js/javascript-lemmatizer/bower_components/underscore/underscore.js",
                "js/javascript-lemmatizer/js/lemmatizer.js",
                "js/PorterStemmer2.js",
                "js/js.js"
            ],
            "css": [
                "css/style.css"
            ]
        }
    ],
    "background": {
        "scripts": [
            "js/jquery-3.4.1.min.js",
            "js/background.js"
        ],
        "persistent": true
    },
    "permissions": [
        "https://*/*",
        "http://*/*",
        "tabs",
        "contextMenus"
    ]
}