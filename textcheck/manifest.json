{
    "manifest_version": 2,
    "name": "Text check",
    "version": "0.1",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["jquery-3.4.1.min.js", "compromise.min.js", "js.js"]
        }
    ]
}