{
    "name": "I am here",
    "version": "1.0",
    "description": "Build an Extension!", 
    "content_scripts": [
        {
          "matches": ["http://*/*", "https://*/*"],
          "js": ["jquery.js","myscript.js"],
          "css":["style.css"]
        }
      ],
      "chrome_url_overrides":{
          "newtab" : "newtab.html"
      },
      "web_accessible_resources":[
        "toolbar.html",
        "style.css"
      ],
    "manifest_version": 2
  }